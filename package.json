{
  "private": true,
  "scripts": {
    "dev": "turbo run dev --concurrency=100",
    "dev-earn-frontend": "turbo run dev --concurrency=100 --filter=@summerfi/earn-protocol... --filter=@summerfi/earn-protocol-landing-page...",
    "dev-rays-frontend": "turbo run dev --concurrency=100 --filter=@summerfi/rays-dashboard...",
    "prebuild": "turbo run prebuild --cache-dir=.turbo",
    "build": "turbo run build --cache-dir=.turbo",
    "build-rays-frontend": "turbo run build --cache-dir=.turbo --filter=@summerfi/rays-dashboard",
    "build-earn-frontend": "turbo run build:apps --cache-dir=.turbo --filter=@summerfi/earn-protocol",
    "build-earn-frontend-landing": "turbo run build:apps --cache-dir=.turbo --filter=@summerfi/earn-protocol-landing-page",
    "prebuild:sdk": "turbo run prebuild --cache-dir=.turbo --filter=./packages/* --filter=./sdk/*",
    "build:sdk": "turbo run build --cache-dir=.turbo --filter=./packages/* --filter=./sdk/*",
    "lint": "turbo run lint --cache-dir=.turbo",
    "lint:sdk": "turbo run lint --cache-dir=.turbo --filter=./packages/* --filter=./sdk/*",
    "lint:fix": "turbo run lint:fix",
    "test": "turbo run test --cache-dir=.turbo",
    "test:sdk": "turbo run test --cache-dir=.turbo --filter=./packages/* --filter=./sdk/*",
    "test:integration": "turbo run test:integration --cache-dir=.turbo",
    "check-circular": "turbo run check-circular --cache-dir=.turbo",
    "db:migrate": "turbo run db:migrate --cache-dir=.turbo",
    "cicheck": "turbo run cicheck --cache-dir=.turbo --filter=./packages/* --filter=./external-api/*  --filter=./summerfi-api/*  && pnpm run coverage:total",
    "cicheck:dev": "turbo run cicheck --cache-dir=.turbo --output-logs=new-only --concurrency=100% --continue",
    "sst:dev": "sst dev --app summerfi-stack",
    "sst:build": "sst build --app summerfi-stack",
    "sst:deploy:dev": "sst deploy --app summerfi-stack",
    "sst:deploy:staging": "sst deploy --stage staging --app summerfi-stack",
    "sst:deploy:prod": "sst deploy --stage production --app summerfi-stack",
    "sst:dev:sdk": "sst dev --app sdk",
    "sst:build:sdk": "sst build --app sdk",
    "sst:deploy:sdk:staging": "sst deploy --stage staging --app sdk",
    "sst:deploy:sdk:prod": "sst deploy --stage production --app sdk",
    "remove": "sst remove",
    "console": "sst console",
    "format": "prettier --check \"./**/*.{ts,tsx,js,jsx,json}\"",
    "format:fix": "prettier --write \"./**/*.{ts,tsx,js,jsx,json}\"",
    "coverage:total": "node scripts/coverage-report/GenerateCoverageReport.js",
    "install:armada": "git submodule update --init armada-protocol/contracts && cd armada-protocol/contracts && pnpm i",
    "build:armada": "cd armada-protocol/contracts && pnpm build && cp -r packages/core-contracts/out/*.sol ../abis/src && cd ../.. && tsx ./scripts/transform-json-to-viem-abi.ts && cd armada-protocol/abis && pnpm genindex && pnpm build"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^5.0.2",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typed-document-node": "^5.0.6",
    "@graphql-codegen/typescript": "^4.0.6",
    "@graphql-codegen/typescript-operations": "^4.2.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@summerfi/eslint-config": "workspace:*",
    "@summerfi/typescript-config": "workspace:*",
    "@tsconfig/node20": "^20.1.4",
    "@turbo/gen": "^2.0.6",
    "@types/aws-lambda": "^8.10.137",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "aws-cdk-lib": "2.132.1",
    "constructs": "10.3.0",
    "create-ts-index": "^1.14.0",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "esbuild": "^0.20.2",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "jest-expect-message": "^1.1.3",
    "jest-extended": "^4.0.2",
    "madge": "^7.0.0",
    "prettier": "^3.2.5",
    "prettier-plugin-solidity": "^1.3.1",
    "sst": "^2.41.5",
    "ts-jest": "^29.1.2",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.16.5",
    "turbo": "^2.1.2",
    "typescript": "^5.6.2",
    "zx": "^8.0.1"
  },
  "name": "summerfi-monorepo",
  "packageManager": "pnpm@8.15.4",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "bignumber.js": "^9.1.2",
    "kysely": "^0.27.3",
    "viem": "2.9.19",
    "zod": "^3.22.4"
  },
  "resolutions": {
    "@emotion/react": "11.11.1",
    "bn.js": "5.2.1",
    "@walletconnect/ethereum-provider": "2.9.1",
    "@ethersproject/abstract-provider": "5.7.0",
    "@ethersproject/signing-key": "5.7.0",
    "acorn": "8.5.0",
    "debug": "4.3.4",
    "eccrypto": "npm:@toruslabs/eccrypto@2.2.1",
    "ethereumjs-util": "7.1.5",
    "mime-db": "1.51.0",
    "mime-types": "2.1.34",
    "query-string": "7.1.3",
    "tslib": "2.2.0",
    "type-fest": "0.20.2",
    "webpack": "5",
    "yargs-parser": "20.2.2"
  }
}
