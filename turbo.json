    "INSTITUTIONS_COGNITO_CLIENT_ID",
    "INSTITUTIONS_COGNITO_CLIENT_SECRET",
    "INSTITUTIONS_SESSION_SECRET",
    "INSTITUTIONS_COGNITO_ADMIN_ACCESS_KEY",
    "INSTITUTIONS_COGNITO_ADMIN_SECRET_ACCESS_KEY",
    "DUNE_LAZYSUMMER_ACCOUNT_API_KEY",
    "ENSO_API_KEY"
  ],
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "prebuild": {
      "dependsOn": ["^prebuild", "^prebuild:nocache"],
      "outputs": [
        "src/types/graphql/generated.ts",
        "src/generated",
        "dist/**",
        "graphql/clients/**",
        "**/*.css.d.ts",
        ".cache/tsbuildinfo.json"
      ]
    },
    "prebuild:nocache": {
      "dependsOn": ["^prebuild:nocache"],
      "cache": false
    },
    "build": {
      "dependsOn": ["prebuild", "^build"],
      "outputs": [
        ".next/**",
        ".next/server/pages/**",
        "!.next/cache/**",
        "dist/**",
        "artifacts/**",
        ".sst/**",
        ".cache/tsbuildinfo.json"
      ]
    },
    "build:apps": {
      "dependsOn": ["prebuild", "^build", "^build:apps"],
      "outputs": [
        ".next/**",
        ".next/server/pages/**",
        "!.next/cache/**",
        "apps/earn-protocol-landing-page/out/**",
        "dist/**",
        "artifacts/**",
        ".sst/**",
        ".cache/tsbuildinfo.json"
      ]
    },
    "test": {
      "dependsOn": ["prebuild", "build", "^test"]
    },
    "test:integration": {
      "dependsOn": ["prebuild", "build", "^test:integration"]
    },
    "lint": {
      "dependsOn": ["prebuild", "build", "^lint"]
    },
    "format": {
      "dependsOn": ["prebuild", "build", "^format"]
    },
    "check-circular": {
      "dependsOn": ["prebuild", "build", "^check-circular"]
    },
    "db:migrate": {
      "dependsOn": ["prebuild"],
      "cache": false
    },
    "cicheck": {
      "dependsOn": [
        "prebuild",
        "build",
        "test",
        "test:integration",
        "lint",
        "check-circular",
        "format"
      ]
    },
    "deploy:staging": {
      "dependsOn": ["cicheck", "^deploy:staging"]
    }
  }
}
