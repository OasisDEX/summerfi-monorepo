{
  "name": "@summerfi/app-ui",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/types/src/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "lint": "eslint *.ts*",
    "generate-module-types": "typed-scss-modules 'src/**/*.module.scss'",
    "generate-module-types-watch": "typed-scss-modules -w 'src/**/*.module.scss'",
    "dev": "pnpm generate-module-types-watch & vite build -w true --mode=dev",
    "build": "pnpm generate-module-types & vite build -w false --mode=prod",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@summerfi/app-token-config": "workspace:*",
    "@summerfi/app-types": "workspace:*",
    "@summerfi/eslint-config": "workspace:*",
    "@summerfi/typescript-config": "workspace:*",
    "@types/loadable__component": "^5.13.9",
    "@types/lodash": "^4.14.109",
    "@types/node": "20.12.7",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "eslint": "8.57.0",
    "glob": "^11.0.0",
    "jest": "29.7.0",
    "sass": "^1.77.8",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vite-plugin-dts": "^4.2.2",
    "vite-plugin-lib-inject-css": "^2.1.1"
  },
  "peerDependencies": {
    "@loadable/component": "^5.16.4",
    "@summerfi/app-icons": "workspace:*",
    "@tabler/icons-react": "^3.11.0",
    "bignumber.js": "^9.1.2",
    "boring-avatars": "1.10.2",
    "clsx": "^2.1.1",
    "lodash": "^4.17.21",
    "next": "^14.2.13",
    "react-dom": "^18.3.1",
    "react": "^18.3.1",
    "usehooks-ts": "^3.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@summerfi/serverless-shared": "workspace:*",
    "@summerfi/app-utils": "workspace:*",
    "include-media": "2.0.0",
    "rollup-preserve-directives": "^1.1.1",
    "sanitize.css": "13.0.0",
    "typed-scss-modules": "^8.0.1",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
