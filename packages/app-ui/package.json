{
  "name": "@summerfi/app-ui",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/types/src/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "lint": "eslint *.ts*",
    "generate-module-types": "typed-scss-modules 'src/**/*.module.scss'",
    "generate-module-types-watch": "typed-scss-modules -w 'src/**/*.module.scss'",
    "dev": "pnpm clean && (pnpm generate-module-types-watch & vite build -w true)",
    "build": "pnpm clean && (pnpm generate-module-types & vite build -w false)",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@summerfi/eslint-config": "workspace:*",
    "@summerfi/typescript-config": "workspace:*",
    "@tabler/icons-react": "^3.4.0",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.2.1",
    "classnames": "^2.5.1",
    "glob": "^10.3.12",
    "sass": "^1.77.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-lib-inject-css": "^2.0.1"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "include-media": "2.0.0",
    "sanitize.css": "13.0.0",
    "typed-scss-modules": "^8.0.1",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
