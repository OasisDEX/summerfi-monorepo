{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "noEmit": true,
    "types": ["node", "jest", "jest-extended"],
    "rootDir": ".",
    "baseUrl": ".",
    "paths": {
      "@summerfi/serverless-shared": ["node_modules/@summerfi/serverless-shared/src/index.ts"],
      "@summerfi/serverless-shared/*": ["node_modules/@summerfi/serverless-shared/src/*"],
      "@summerfi/sdk-common/*": ["node_modules/@summerfi/sdk-common/src/*"],
      "@summerfi/common": ["node_modules/@summerfi/common/src/index.ts"],
      "~swap-service": ["src/index.ts"],
      "~swap-service/*": ["src/*"],
      "~sdk-common/*": ["node_modules/@summerfi/sdk-common/src/*"],
      "~src/*": ["src/*"]
    }
  },
  "ts-node": {
    "compilerOptions": {
      "module": "ES2022",
      "target": "ES2022"
    }
  }
}
