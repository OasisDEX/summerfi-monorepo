{
  "name": "@summerfi/app-earn-ui",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "type": "module",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist/src"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "lint": "eslint *.ts*",
    "generate-module-types": "concurrently \"tcm -s -p 'src/**/*.module.css'\" \"tcm --outDir 'dist/' -s -p 'src/**/*.module.css'\"",
    "generate-module-types-watch": "concurrently \"tcm -w -s -p 'src/**/*.module.css'\" \"tcm --outDir 'dist/' -w -s -p 'src/**/*.module.css'\"",
    "build": "concurrently \"pnpm vite build -w false\" \"pnpm generate-module-types\"",
    "prebuild": "concurrently \"pnpm vite build -w false\" \"pnpm generate-module-types\"",
    "dev": "concurrently \"pnpm vite build -w true --mode=dev\" \"pnpm:generate-module-types-watch\"",
    "knip": "knip"
  },
  "devDependencies": {
    "@summerfi/app-icons": "workspace:*",
    "@summerfi/app-token-config": "workspace:*",
    "@summerfi/app-types": "workspace:*",
    "@summerfi/app-utils": "workspace:*",
    "@summerfi/eslint-config": "workspace:*",
    "@summerfi/typescript-config": "workspace:*",
    "@types/loadable__component": "^5.13.9",
    "@types/lodash-es": "^4.17.1",
    "@types/node": "20.12.7",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "concurrently": "^9.0.1",
    "eslint": "8.57.0",
    "glob": "^11.0.0",
    "jest": "29.7.0",
    "knip": "5.64.0",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.7.3",
    "unplugin-isolated-decl": "^0.13.5",
    "vite": "7.0.8",
    "vite-plugin-lib-inject-css": "^2.2.1"
  },
  "peerDependencies": {
    "@account-kit/core": "^4.52.2",
    "@account-kit/infra": "^4.52.2",
    "@account-kit/react": "^4.52.2",
    "@loadable/component": "^5.16.4",
    "@summerfi/armada-protocol-common": "workspace:*",
    "@summerfi/summer-earn-rates-subgraph": "workspace:*",
    "@tanstack/react-query": "^5.51.1",
    "bignumber.js": "^9.3.0",
    "boring-avatars": "^2.0.0",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.12",
    "embla-carousel-react": "8.3.0",
    "embla-carousel": "8.3.0",
    "lodash-es": "^4.17.1",
    "next": "15.5.9",
    "react-animate-height": "^3.2.3",
    "react-dom": "19.1.0",
    "react": "19.1.0",
    "react-day-picker": "9.9.0",
    "react-toastify": "^11.0.3",
    "uniqolor": "1.1.1",
    "usehooks-ts": "^3.1.0",
    "viem": "2.29.2",
    "wagmi": "2.10.9"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@number-flow/react": "^0.5.10",
    "rollup-preserve-directives": "^1.1.3",
    "sanitize.css": "13.0.0",
    "typed-css-modules": "0.9.1",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
