{
  "name": "@summerfi/app-server-handlers",
  "version": "0.0.1",
  "license": "Apache-2.0",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/types/*.d.ts",
      "import": "./dist/*/index.js",
      "require": "./dist/*/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint *.ts*",
    "dev": "pnpm vite build -w true --mode=dev",
    "build": "pnpm vite build -w false --mode=prod",
    "prebuild": "pnpm vite build -w false --mode=prod",
    "clean": "rm -rf dist",
    "knip": "knip"
  },
  "devDependencies": {
    "@summerfi/app-types": "workspace:*",
    "@summerfi/eslint-config": "workspace:*",
    "@summerfi/typescript-config": "workspace:*",
    "@types/lodash-es": "^4.17.1",
    "@types/node": "^22.14.1",
    "eslint": "8.57.0",
    "glob": "^11.0.0",
    "knip": "^5.50.4",
    "typescript": "^5.7.3",
    "unplugin-isolated-decl": "^0.13.5",
    "vite": "7.0.5",
    "vite-plugin-node-polyfills": "^0.23.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@summerfi/summer-earn-rates-subgraph": "workspace:*",
    "@summerfi/app-utils": "workspace:*",
    "@summerfi/sdk-client": "workspace:*",
    "@summerfi/sdk-common": "workspace:*",
    "bignumber.js": "^9.1.2",
    "dayjs": "^1.11.12",
    "graphql-request": "^6.1.0",
    "mixpanel": "^0.18.0",
    "jose": "^6.0.10",
    "lodash-es": "^4.17.1",
    "next": "15.4.4",
    "viem": "2.29.2"
  },
  "dependencies": {
    "rollup-preserve-directives": "^1.1.3",
    "vite-tsconfig-paths": "^5.1.3"
  }
}
