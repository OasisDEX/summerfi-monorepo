{
  "name": "@summerfi/earn-protocol",
  "version": "0.0.6",
  "license": "Apache-2.0",
  "scripts": {
    "prebuild": "concurrently \"pnpm:codegen\" \"pnpm:generate-module-types\"",
    "dev": "dotenv -e ../../.env -e ../../.env.local -- concurrently \"next dev -p 3002 --turbopack\" \"pnpm:generate-module-types-watch\"",
    "build:apps": "dotenv -e ../../.env -e ../../.env.local -- next build",
    "start": "dotenv -e ../../.env -e ../../.env.local -- next start -p 3002",
    "lint": "tsc --noEmit && eslint .",
    "lint:fix": "eslint . --fix",
    "lint-timing": "TIMING=1 eslint .",
    "codegen": "DOTENV_CONFIG_PATH=../../.env graphql-codegen -r dotenv/config",
    "update-tables": "dotenv -e ../../.env -e ../../.env.local -- node --import tsx misc/scripts/update-tables.ts",
    "knip": "knip",
    "knip-fix": "knip --fix",
    "generate-module-types": "tcm -s -p './+(components|features|app)/**/*.module.css'",
    "generate-module-types-watch": "tcm -p './+(components|features|app)/**/*.module.css' -w -s"
  },
  "dependencies": {
