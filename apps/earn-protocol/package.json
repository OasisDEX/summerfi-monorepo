    "@summerfi/app-server-handlers": "workspace:*",
    "@summerfi/app-tos": "workspace:*",
    "@summerfi/app-types": "workspace:*",
    "@summerfi/app-utils": "workspace:*",
    "@summerfi/armada-protocol-abis": "workspace:*",
    "@summerfi/armada-protocol-common": "workspace:*",
    "@summerfi/sdk-client-react": "workspace:^",
    "@summerfi/sdk-client": "workspace:^",
    "@summerfi/sdk-common": "workspace:*",
    "@summerfi/serverless-shared": "workspace:*",
    "@summerfi/subgraph-manager-common": "workspace:*",
    "@summerfi/summer-beach-club-db": "workspace:*",
    "@summerfi/summer-protocol-db": "workspace:*",
    "@tanstack/react-query": "^5.51.1",
    "@transak/transak-sdk": "^3.1.3",
