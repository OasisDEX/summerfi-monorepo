{
  "name": "@summerfi/earn-protocol-landing-page",
  "version": "0.0.1",
  "license": "Apache-2.0",
  "scripts": {
    "prebuild": "concurrently \"pnpm:codegen\" \"pnpm:generate-module-types\"",
    "dev": "dotenv -e ../../.env -e ../../.env.local -- concurrently \"next dev -p 3003 --turbopack\" \"pnpm:generate-module-types-watch\"",
    "build:apps": "dotenv -e ../../.env -e ../../.env.local -- next build",
    "start": "next start -p 3003",
    "lint": "eslint .",
    "codegen": "DOTENV_CONFIG_PATH=../../.env graphql-codegen -r dotenv/config",
    "knip": "knip",
    "generate-module-types": "tcm -s -p './+(components|features|app)/**/*.module.css'",
    "generate-module-types-watch": "tcm -p './+(components|features|app)/**/*.module.css' -w -s"
  },
  "dependencies": {
    "@loadable/component": "^5.16.4",
    "@summerfi/app-earn-ui": "workspace:*",
    "@summerfi/app-types": "workspace:*",
    "@summerfi/app-utils": "workspace:*",
    "@summerfi/sdk-client": "workspace:^",
    "@summerfi/sdk-client-react": "workspace:^",
    "@summerfi/sdk-common": "workspace:*",
    "browser-detect": "^0.2.28",
    "clsx": "^2.1.1",
    "concurrently": "^9.0.1",
    "graphql-request": "^6.1.0",
    "graphql-tag": "^2.12.6",
    "lodash-es": "^4.17.1",
    "mixpanel-browser": "^2.53.0",
    "next": "15.3.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sharp": "^0.33.5",
    "usehooks-ts": "^3.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@summerfi/eslint-config": "workspace:*",
    "@summerfi/typescript-config": "workspace:*",
    "@types/loadable__component": "^5.13.9",
    "@types/lodash-es": "^4.17.1",
    "@types/mixpanel-browser": "^2.49.0",
    "@types/node": "^20.14.2",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "graphql": "^16.8.1",
    "knip": "^5.50.4",
    "typed-css-modules": "0.9.1",
    "typescript": "^5.7.3"
  },
  "packageManager": "pnpm@8.15.4"
}
