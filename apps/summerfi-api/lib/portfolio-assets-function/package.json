{
  "name": "@summerfi/portfolio-assets-function",
  "version": "1.0.0",
  "scripts": {
    "tsc": "tsc",
    "watch": "tsc -w",
    "test": "jest --passWithNoTests",
    "build": "tsc -b -v",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "docker:build": "tar chzf files.tar.gz Dockerfile node_modules dist && docker build --platform linux/amd64 -t portfolio-assets - < files.tar.gz && rm files.tar.gz",
    "docker:run": "docker run --rm -p 9000:8080 portfolio-assets",
    "dev": "npm run tsc && npm run docker:build && npm run docker:run",
    "invoke": "curl http://localhost:9000/2015-03-31/functions/function/invocations -d '{\"queryStringParameters\": {\"address\": \"0x10649c79428d718621821Cf6299e91920284743F\"}}' | jq",
    "aws:deploy": "deploy zip file"
  },
  "devDependencies": {
    "@summerfi/eslint-config": "workspace:*",
    "@summerfi/typescript-config": "workspace:*",
    "@types/node": "^20.11.5"
  },
  "dependencies": {
    "node-fetch": "^3.3.2",
    "@summerfi/serverless-shared": "workspace:*",
    "zod": "^3.22.4"
  }
}
