{
  "name": "@summerfi/sdk-client",
  "version": "0.0.1",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.d.ts"
    },
    "./*": {
      "import": "./src/*/index.ts",
      "types": "./src/*/index.d.ts"
    }
  },
  "scripts": {
    "tsc": "tsc",
    "watch": "tsc -w",
    "test": "jest --coverage=true",
    "testw": "jest --watch",
    "build": "tsc -b --preserveWatchOutput -v tsconfig.build.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "bundle:clean": "rm -rf bundle/dist bundle/tsconfig.bundle.tsbuildinfo .cache/tsbuildinfo.json tsconfig.bundle.tsbuildinfo",
    "bundle:js": "esbuild src/index.ts --platform=neutral --target=es2020 --conditions= --main-fields=main --format=cjs --bundle --sourcemap --outfile=bundle/dist/index.js --keep-names --external:crypto --external:fs --external:http --external:https --external:net --external:os --external:path --external:stream --external:tls --external:tty --external:zlib --external:bignumber.js --external:@cowprotocol/cow-sdk --external:@ethersproject/abstract-signer --external:@trpc/client",
    "declarations": "tsc --emitDeclarationOnly -p tsconfig.bundle.json",
    "bundle:types": "pnpm declarations && tsx scripts/generate-declarations.ts",
    "bundle:npm": "pnpm bundle:clean && pnpm bundle:js && pnpm bundle:types",
    "publish:npm": "pnpm bundle:npm && cd bundle && npm publish && cd .."
  },
  "dependencies": {
    "@cowprotocol/cow-sdk": "^6.3.2",
    "@ethersproject/abstract-signer": "^5.8.0",
    "@summerfi/armada-protocol-common": "workspace:*",
    "@summerfi/armada-protocol-service": "workspace:*",
    "@summerfi/protocol-plugins": "workspace:*",
    "@summerfi/sdk-common": "workspace:*",
    "@summerfi/sdk-server": "workspace:*",
    "@summerfi/subgraph-manager-common": "workspace:*",
    "@trpc/client": "11.0.0-rc.844"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.52.13",
    "@summerfi/eslint-config": "workspace:*",
    "@summerfi/jest-config": "workspace:*",
    "@summerfi/testing-utils": "workspace:*",
    "@summerfi/typescript-config": "workspace:*",
    "tsx": "^4.19.4"
  }
}
