{
  "extends": "@summerfi/typescript-config/tsconfig.test.json",
  "compilerOptions": {
    "rootDir": "src",
    "baseUrl": ".",
    "paths": {
      "@summerfi/order-planner-common/*": ["node_modules/@summerfi/order-planner-common/src/*"],
      "@summerfi/sdk-common/*": ["node_modules/@summerfi/sdk-common/src/*"],
      "@summerfi/core-contracts": ["node_modules/@summerfi/core-contracts/src/index.ts"],
      "@summerfi/swap-common/*": ["node_modules/@summerfi/swap-common/src/*"],
      "@summerfi/deployment-utils": ["node_modules/@summerfi/deployment-utils/src/index.ts"],
      "@summerfi/protocol-plugins": ["node_modules/@summerfi/protocol-plugins/src/index.ts"],
      "@summerfi/protocol-plugins-common": [
        "node_modules/@summerfi/protocol-plugins-common/src/index.ts"
      ],
      "@summerfi/tokens-service": ["node_modules/@summerfi/tokens-service/src/index.ts"],
      "@summerfi/blockchain-client-provider": [
        "node_modules/@summerfi/blockchain-client-provider/src/index.ts"
      ]
    }
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules", "dist"]
}
