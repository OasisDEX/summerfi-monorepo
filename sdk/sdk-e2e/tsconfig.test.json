{
  "extends": "@summerfi/typescript-config/tsconfig.test.json",
  "compilerOptions": {
    "rootDir": "src",
    "baseUrl": ".",
    "paths": {
      "@summerfi/sdk-common/*": ["node_modules/@summerfi/sdk-common/src/*"],
      "~sdk-common/*": ["node_modules/@summerfi/sdk-common/src/*"],
      "@summerfi/sdk-client": ["node_modules/@summerfi/sdk-client/src"],
      "~sdk-client/*": ["node_modules/@summerfi/sdk-client/src/*"]
    }
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
