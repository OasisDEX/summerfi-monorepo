{
  "name": "@summerfi/subgraph-manager-common",
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.d.ts"
    }
  },
  "scripts": {
    "tsc": "tsc",
    "watch": "tsc -w",
    "prebuild": "pnpm run generate",
    "build": "tsc -b --preserveWatchOutput tsconfig.build.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "generate": "pnpm run generate:protocol && pnpm run generate:institutions",
    "generate:protocol": "DOTENV_CONFIG_PATH=../../.env graphql-codegen --config summer-protocol.yml -r dotenv/config",
    "generate:institutions": "DOTENV_CONFIG_PATH=../../.env graphql-codegen --config summer-institutions.yml -r dotenv/config"
  },
  "dependencies": {
    "@summerfi/sdk-common": "workspace:*",
    "graphql-request": "^6.1.0",
    "graphql-tag": "^2.12.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@summerfi/eslint-config": "workspace:*",
    "@summerfi/jest-config": "workspace:*",
    "@summerfi/typescript-config": "workspace:*",
    "graphql": "^16.8.1"
  }
}
